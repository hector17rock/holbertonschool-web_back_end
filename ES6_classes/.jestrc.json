{
  "testEnvironment": "node",
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "moduleFileExtensions": ["js", "json"],
  "testMatch": [
    "**/__tests__/**/*.js",
    "**/?(*.)+(spec|test).js"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/coverage/"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!(.*\\.mjs$))"
  ],
  "clearMocks": true,
  "verbose": true,
  "testTimeout": 10000
}
